<?xml version="1.1" encoding="UTF-8"?>
<schemalist gettext-domain="project">
    <schema id="org.gnome.shell.extensions.prjchange" path="/org/gnome/shell/extensions/prjchange/">
        
        <key type="as" name="switch-projects">
            <default><![CDATA[['<Super>less']]]></default>
            <summary>Circle to next project</summary>
        </key>

        <key type="as" name="switch-projects-backward">
            <default><![CDATA[['<Super><Shift>less']]]></default>
            <summary>Circle to previous project</summary>
        </key>

         <key name="show-indicator" type="b">
            <default>true</default>
        </key>
    </schema>
</schemalist>